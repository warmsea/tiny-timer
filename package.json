{
  "name": "tiny-timer",
  "productName": "tiny-timer",
  "version": "1.0.0",
  "description": "A tiny timer stays on the screen",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx . && prettier --write .",
    "test": "jest"
  },
  "keywords": [],
  "author": {
    "name": "Su Su",
    "email": "s@warmsea.net"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tiny_timer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.ts",
            "renderer": {
              "config": "./webpack.renderer.config.ts",
              "entryPoints": [
                {
                  "name": "main_window",
                  "html": "./src/renderer/main/main.html",
                  "js": "./src/renderer/main/main.tsx",
                  "preload": {
                    "js": "./src/renderer/main/preload.ts"
                  }
                },
                {
                  "name": "display_window",
                  "html": "./src/renderer/display/display.html",
                  "js": "./src/renderer/display/display.tsx",
                  "preload": {
                    "js": "./src/renderer/display/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/jest": "^26.0.20",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "css-loader": "^5.2.4",
    "electron": "12.0.6",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-loader": "^2.0.0",
    "prettier": "2.2.1",
    "sass": "^1.32.7",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "webpack": "^5.21.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
